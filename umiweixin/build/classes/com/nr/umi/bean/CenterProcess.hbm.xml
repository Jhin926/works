<?xml version="1.0" encoding='UTF-8'?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
 
<hibernate-mapping package="com.nr.umi.bean">
<class name="CenterProcess" table="t_process"  dynamic-insert="true" dynamic-update="true">
	<id name="id" column="c_id">
		<generator class="native" />
	</id>
	<property name="srcID" column="c_srcID"></property>
	<property name="dstID" column="c_dstID"></property>
	<property name="userID" column="c_userID" ></property>
	<property name="cashID" column="c_cashID"></property>
	<property name="orderNo" column="c_orderNo"></property>
	<property name="amount" column="c_amount"></property>
	<property name="dstCashID" column="c_dst_cashID"></property>
</class>
</hibernate-mapping>