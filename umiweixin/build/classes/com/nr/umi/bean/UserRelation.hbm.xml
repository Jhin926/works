<?xml version="1.0" encoding='UTF-8'?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
 
<hibernate-mapping package="com.nr.umi.bean">
<class name="UserRelation" table="t_user_relation">
	<id name="id" column="c_id">
	</id>
	<property name="userId" column="c_user_id"></property>
	<property name="userStatus" column="c_user_status"></property>
	<property name="reward" column="c_reward"></property>
	<property name="qualificationReward" column="c_qualification_reward"></property>
	<property name="registerReward" column="c_register_reward"></property>
	<property name="investReward" column="c_invest_reward"></property>
	<property name="timestamp" column="c_timestamp" type="timestamp"></property>
</class>
</hibernate-mapping>