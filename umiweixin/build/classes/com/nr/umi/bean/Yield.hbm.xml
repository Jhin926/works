<?xml version="1.0" encoding='UTF-8'?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
 
<hibernate-mapping package="com.nr.umi.bean">
<class name="Yield" table="t_sys_fund_rate" dynamic-insert="true" dynamic-update="true">
	<id name="id" column="c_id">
	<generator class="native"></generator>
	</id>
	<property name="fundCode" column="c_fund_code"></property>
	<property name="date" column="c_date" type="date" ></property>
	<property name="dRate" column="c_7d_rate"></property>
	<property name="kReturn" column="c_10k_return"></property>
</class>
</hibernate-mapping>