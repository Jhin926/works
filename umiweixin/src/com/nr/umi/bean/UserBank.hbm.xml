<?xml version="1.0" encoding='UTF-8'?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.nr.umi.bean">
	<class name="UserBank" table="t_user_bank_info" dynamic-insert="true"
		dynamic-update="true">
		<id name="id" column="c_id">
			<generator class="native"></generator>
		</id>
		<property name="user_id" column="c_user_id">
		</property>
		<property name="bank_name" column="c_bank_name"></property>
		<property name="province" column="c_province"></property>
		<property name="city" column="c_city"></property>
		<property name="branch" column="c_branch"></property>
		<property name="card_no" column="c_card_no"></property>
		<property name="account_name" column="c_account_name"></property>
		<property name="card_id" column="c_card_id"></property>
		<property name="isVerified" column="c_is_verified"></property>
		<property name="bankCode" column="c_bank_code"></property>
		<property name="reversedPhone" column="c_reserved_phone"></property>
	</class>
</hibernate-mapping>