<?xml version="1.0" encoding='UTF-8'?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
 
<hibernate-mapping package="com.nr.umi.bean">
<class name="UserSession" table="t_session"  dynamic-insert="true" dynamic-update="true">
	<id name="id" column="c_id">
	<generator class="native"></generator>
	</id>
	<property name="ticket" column="c_ticket"></property>
	<property name="freshTime" column="c_timestamp" ></property>
</class>
</hibernate-mapping>