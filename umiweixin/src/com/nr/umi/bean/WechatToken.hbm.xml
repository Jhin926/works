<?xml version="1.0" encoding='UTF-8'?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
 
<hibernate-mapping package="com.nr.umi.bean">
<class name="WechatToken" table="t_wechat_token" dynamic-insert="true" dynamic-update="true">
	<id name="id" column="c_id">
	<generator class="native"></generator>
	</id>
	<property name="accessToken" column="c_access_token"></property>
	<property name="openid" column="c_openid"></property>
	<property name="time" column="c_time"></property>
	<property name="expiresIn" column="c_expires_in"></property>
	<property name="userID" column="c_user_id"></property>
	<property name="code" column="c_code"></property>
	<property name="refreshToken" column="c_refresh_token"></property>
	<property name="ret" column="c_ret"></property>
	<property name="amount" column="c_amount"></property>
</class>
</hibernate-mapping>